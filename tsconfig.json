{
    "compilerOptions": {
      "target": "ES2020", // または 'ES6'
      "lib": ["ES2020", "DOM"], // 必要なライブラリを含める
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist", 
      "rootDir": "./src"  
    }
  }
  